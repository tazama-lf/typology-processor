{
  "name": "typology-processor",
  "version": "1.2.0",
  "description": "typology-processor",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "clean": "npx rimraf build node_modules coverage package-lock.json",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:eslint-unused": "eslint --report-unused-disable-directives \"**/*.ts\"",
    "lint:eslint-no-inline": "eslint --no-inline-config \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --config=jest.config.ts --passWithNoTests --forceExit",
    "test:watch": "jest --config=jest.config.ts --watch",
    "prepare": "husky"
  },
  "keywords": [
    "mojaloop",
    "tazama",
    "frm"
  ],
  "contributors": [
    {
      "name": "Johan Foley"
    },
    {
      "name": "Joey GÃ¶ksu"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@cortex-js/compute-engine": "0.20.2",
    "@tazama-lf/frms-coe-lib": "4.2.0-rc.1",
    "@tazama-lf/frms-coe-startup-lib": "2.3.0-rc.1",
    "arangojs": "^8.8.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "node-cache": "^5.1.2",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^52.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "husky": "9.0.11",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
