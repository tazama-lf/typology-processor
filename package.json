{
  "name": "typology-processor",
  "version": "1.2.0",
  "description": "typology-processor",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf build dist template jest.config.js jest.config.js.map node_modules",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "test": "jest --config=jest.config.ts --passWithNoTests --forceExit",
    "test:watch": "jest --config=jest.config.ts --watch",
    "prepare": "husky install"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Johan Foley"
    },
    {
      "name": "Joey GÃ¶ksu"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "^2.2.3",
    "@frmscoe/frms-coe-startup-lib": "^2.1.0",
    "@log4js-node/logstash-http": "^1.1.0",
    "arangojs": "^8.4.0",
    "dotenv": "^16.0.0",
    "ioredis": "^5.3.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.0",
    "node-cache": "^5.1.2",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/arangodb": "^3.5.11",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.172",
    "@types/log4js": "^2.3.5",
    "@types/node": "^16.9.1",
    "@types/nodemon": "^1.19.1",
    "@types/prettier": "^3.0.0",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "8.0.3",
    "ioredis-mock": "^8.8.1",
    "jest": "^29.2.2",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.20",
    "prettier": "^3.1.1",
    "rimraf": "^3.0.2",
    "tplant": "^3.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
